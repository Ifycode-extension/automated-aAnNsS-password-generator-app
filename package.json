{
  "name": "automated-aannss-password-generator-app",
  "version": "1.0.0",
  "description": "Google Africa Developer Scholarship workshop exercise for building a javascript dev environment course",
  "homepage": "https://ifycode.github.io/Automated-aAnNsS-Password-Generator-App",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt -h \"http://serverless.social\" -p 3000 --subdomain ifycode",
    "share": "npm-run-all --parallel open:src localtunnel",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "copy": "copyfiles -f ./src/img/* ./dist/img",
    "imagecopy": "babel-node buildScripts/imageCopyMessage.js",
    "prebuild": "npm-run-all clean-dist copy imagecopy lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "dev": "lite-server",
    "deploy": "surge ./dist"
  },
  "author": "App by Obiagba Mary (Dev environment from Cory House's building a javascript dev environment course)",
  "license": "MIT",
  "dependencies": {
    "copy-webpack-plugin": "^6.0.4",
    "copyfiles": "^2.3.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "lite-server": "^2.5.4",
    "localtunnel": "1.8.1",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "surge": "0.20.4",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
